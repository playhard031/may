                    <script src="../js/jquery.validate.min.js"></script>
<script>
    $(document).ready(function() {
        var forms = $(this).find('form.js-validate');

        var removeTabIndex = function() {
            $(this).removeAttr('tabIndex');
        };

        var accessibleFocus = function(element) {
            element
                .attr('tabIndex', '-1')
                .focus()
                .bind({
                    'blur': removeTabIndex(),
                    'focusout': removeTabIndex()
                })
        };

        $(forms).each( function() {
            $(this).validate({
                errorClass: 'form__message--invalid',
                errorElement: 'span',
                highlight: function(element, errorClass, validClass) {
                    element.classList.add('form__state--invalid');
                },
                unhighlight: function(element, errorClass, validClass) {
                    element.classList.remove('form__state--invalid');
                },
                errorPlacement: function(error, element) {
                    if (element.attr('type') === 'radio') {
                        element = $(element).parent().parent();
                    }
                    $(element).before(error);
                },
                normalizer: function(value) {
                    return value ? value.trim() : '';
                },
                submitHandler: function(form) {
                    var formContainer = $(form).closest('.js-form-container'),
                        display = formContainer.find('.js-form-display'),
                        success = formContainer.find('.js-form-success'),
                        error = formContainer.find('.js-form-error'),
                        button = formContainer.find('.js-form-button'),
                        gtmForm = form.getAttribute('data-gtm-form');

                    button.addClass('is-disabled');

                    $.ajax({
                        url: $(form).data('path'),
                        method: $(form).attr('method'),
                        data: $(form).serialize(),

                        success: function(response) {
                            display.hide();
                            error.hide();
                            success.show();
                            success.attr('aria-hidden', 'false');

                            $('html, body').animate({
                                scrollTop: success.offset().top - 80
                            }, 700);

                            if (gtmForm) {
                                dataLayer.push({
                                    'event': 'GAEvent',
                                    'eventCategory': gtmForm,
                                    'eventAction': 'Send',
                                    'eventLabel': '/assets/img/stripe.png',
                                });
                            };

                            accessibleFocus(success);
                        },
                        error: function(response) {
                            display.hide();
                            success.hide();
                            error.show();
                            error.attr('aria-hidden', 'false');

                            $('html, body').animate({
                                scrollTop: error.offset().top - 80
                            }, 700);

                            if (gtmForm) {
                                dataLayer.push({
                                    'event': 'GAEvent',
                                    'eventCategory': gtmForm,
                                    'eventAction': 'Error Server',
                                    'eventLabel': '/assets/img/stripe.png',
                                });
                            }

                            accessibleFocus(error);
                        }
                    });
                }
            });
        });
    });
</script>
